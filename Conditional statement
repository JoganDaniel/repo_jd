import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
     Scanner s=new Scanner(System.in);
     int a=10,b=5,c=7;
     if(a>b && a>c)
     {
    	 System.out.println(a+" is >");
     }
     else if(b>a && b>c)
     {
    	 System.out.println(b+" is >");
     }
     else
     {
    	 System.out.println(c+" is >");
     }
	int d=(c>b) ? c : b;
	System.out.print(d +" is >");
	}
}
Output:
10 is >
7 is >
